{
  "rewrites": [
    { "source": "/check", "destination": "/check.html" },
    { "source": "/api/stripe-webhook", "destination": "/api/ops?action=stripe-webhook" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
