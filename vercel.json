{
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/router.js" },
    { "src": "^/watch$", "dest": "/watch.html" },
    { "src": "^/$", "status": 302, "headers": { "Location": "/site/" } }
  ],
  "headers": [
    { "source": "/mags/(.*)", "headers": [{ "key": "X-Robots-Tag", "value": "noindex, nofollow" }] }
  ]
}
