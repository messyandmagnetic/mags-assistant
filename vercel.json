{
  "functions": {
    "api/*.ts": { "runtime": "nodejs18.x" }
  },
  "rewrites": [
    { "source": "/check", "destination": "/check.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ]
}
